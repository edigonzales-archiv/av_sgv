  <step>
    <name>Area neu berechnen</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>xanadu2</connection>
    <execute_each_row>Y</execute_each_row>
    <replace_variables>Y</replace_variables>
    <sql>DELETE FROM av_sgv_work.liegen_selbst_area_parts WHERE bfsnr = ?;

INSERT INTO av_sgv_work.liegen_selbst_area_parts (nbident, nummer, geometrie, art, bfsnr)

SELECT liegen.nbident,
 CASE WHEN selbst.ogc_fid IS NULL 
  THEN liegen.nummer
  ELSE selbst.nummer
 END as nummer,
 geom,
 CASE WHEN selbst.ogc_fid IS NULL 
  THEN &apos;liegen&apos;
  ELSE &apos;selbst&apos;
 END as art,
 liegen.bfsnr
FROM
(
 SELECT ST_PointOnSurface(geom) as geom_point, geom
 FROM
 (
  SELECT (ST_Dump(ST_Polygonize(geom))).geom as geom
  FROM
  (
   SELECT ST_Union(geom) as geom
   FROM
   (
    SELECT ST_Boundary((geometrie)) as geom
    FROM av_mopublic.liegenschaften__selbstrecht_bergwerk
    WHERE bfsnr = ?
    AND grundstuecksart &lt;&gt; &apos;Quellenrecht&apos;

    UNION

    SELECT ST_Boundary((geometrie)) as geom
    FROM av_mopublic.liegenschaften__liegenschaft
    WHERE bfsnr = ?
   ) as a
  ) as b
 ) as c
 WHERE geometrytype(geom) LIKE &apos;%POLYGON&apos;
) as d
LEFT JOIN 
( 
 SELECT *
 FROM av_mopublic.liegenschaften__liegenschaft 
 WHERE bfsnr = ?
) as liegen ON ST_Within(d.geom_point, liegen.geometrie)
LEFT JOIN 
(
 SELECT * 
 FROM av_mopublic.liegenschaften__selbstrecht_bergwerk
 WHERE bfsnr = ?
 AND grundstuecksart &lt;&gt; &apos;Quellenrecht&apos;
) as selbst ON ST_Within(d.geom_point, selbst.geometrie)
WHERE ST_Area(geom) &gt; 0.01;</sql>

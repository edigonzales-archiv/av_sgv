  <step>
    <name>BB Gebäude</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>xanadu2</connection>
    <execute_each_row>Y</execute_each_row>
    <replace_variables>Y</replace_variables>
    <sql>DELETE FROM av_sgv_work.gebaeude_rechte WHERE bfsnr = ?;

INSERT INTO av_sgv_work.gebaeude_rechte (nummer, art_recht, art_geb, nbident, bfsnr, geb_id, geometrie, grundbuch, gemeinde, gb_gemnr)


SELECT gebaeude.nummer, gebaeude.art, &apos;Gebaeude&apos;::varchar as art_geb, gebaeude.nbident, gebaeude.bfsnr, gebaeude.geb_id, gebaeude.geometrie, kreise.grundbuch, kreise.gemeinde, kreise.gb_gemnr
FROM
(
 SELECT *
 FROM
 (
  SELECT rechte.nummer, rechte.art, rechte.nbident, rechte.bfsnr, bb.ogc_fid as geb_id, ST_CollectionExtract(ST_Multi(ST_Intersection(bb.geometrie, rechte.geometrie)),3) as geometrie
  FROM
  (
   SELECT ogc_fid, geometrie
   FROM av_mopublic.bodenbedeckung__boflaeche
   WHERE art = &apos;Gebaeude&apos;
  AND bfsnr = ?
  ) as bb,
  (
   SELECT *
   FROM av_sgv_work.liegen_selbst_area
   WHERE bfsnr = ?
  ) as rechte
  WHERE ST_Intersects(bb.geometrie, rechte.geometrie)
 ) as a
 WHERE geometrytype(geometrie) LIKE &apos;%POLYGON&apos;
) as gebaeude, 
(
 SELECT *
 FROM av_grundbuch.grundbuchkreise 
 WHERE gem_bfs = ?
) as kreise
WHERE ST_Within(ST_PointOnSurface(gebaeude.geometrie), kreise.wkb_geometry);</sql>
